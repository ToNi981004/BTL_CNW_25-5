
@{
    ViewBag.Title = "ThanhToan";
    Layout = "~/Views/Shared/_LayoutNguoiDung.cshtml";
}

    <div id="page-wrapper">
        <div class="container-fluid">
            <div class="row">
                <div class="col-lg-12">
                    <h1 class="page-header">Thông Tin Giao Hàng</h1>
                    @*Biểu tượng người*@
                </div>
                <div class="hero-widget well well-sm">
                    <div class="icon">
                        <i class="glyphicon glyphicon-user"></i>
                    </div>
                    <div class="text">
                        <span class="value"><i class="fa fa-github-alt"></i></span>

                    </div>

                </div>
                
                @*Thông Tin người dùng*@
                <div class="panel panel-green">
                    <div class="panel-heading">
                        <h4 class="panel-title">
                            <i class="fa fa-outdent"></i>   Thông Tin
                        </h4>
                    </div>
                    <div id="collapseOne" class="panel-collapse collapse in">
                        <div class="form-group">
                            <label><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">Tên Sản Phẩm:</font></font></label>
                            <input class="form-control" id="disabledInput" type="text" placeholder="" disabled="">
                        </div>
                        <div class="form-group">
                            <label><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">Số Lượng:</font></font></label>
                            <input class="form-control" id="disabledInput" type="text" placeholder="Số Lượng" disabled="">
                        </div>
                        <div class="form-group">
                            <label><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">Giá Sản Phẩm/1 Cái:</font></font></label>
                            <input class="form-control" id="disabledInput" type="text" placeholder="Giá" disabled="">
                        </div>
                        <div class="form-group">
                            <label><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">Tên Tài Khoản:</font></font></label>
                            <input class="form-control" id="disabledInput" type="text" placeholder="" disabled="">
                        </div>

                        <div class="form-group">
                            <label><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">Giới Tính:</font></font></label>
                            <input class="form-control" id="disabledInput" type="text" placeholder="giới tính" disabled="">
                        </div>
                        <div class="form-group">
                            <label><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">Số Điện Thoại:</font></font></label>
                            <input class="form-control" id="disabledInput" type="text" placeholder="số điện thoại" disabled="">
                        </div>
                        <div class="form-group">
                            <label><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">Địa Chỉ:</font></font></label>
                            <input class="form-control" id="disabledInput" type="text" placeholder="địa chỉ" disabled="">
                        </div>
                        <div class="form-group">
                            <label><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">Quận/Huyện:</font></font></label>
                            <input class="form-control" id="disabledInput" type="text" placeholder="quận - huyện" disabled="">
                        </div>
                        <div class="form-group">
                            <label><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">Thành Phố:</font></font></label>
                            <input class="form-control" id="disabledInput" type="text" placeholder="thành phố" disabled="">
                        </div>
                        <div class="form-group">
                            <label><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">Tên Công Ty:</font></font></label>
                            <input class="form-control" id="disabledInput" type="text" placeholder="tên công ty" disabled="">
                        </div>
                        <div class="form-group">
                            <label><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">Địa chỉ Email:</font></font></label>
                            <input class="form-control" id="disabledInput" type="text" placeholder="Email" disabled="">
                        </div>

                    </div>
                </div>

                <!-- /.col-lg-12 -->
            </div>
            <!-- /.row -->
        </div>
        <!-- /.container-fluid -->
    </div>

